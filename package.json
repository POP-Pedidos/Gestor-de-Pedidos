{
  "name": "pop-pedidos",
  "productName": "POP Pedidos",
  "publisherName": "POP Pedidos",
  "appId": "poppedidos.com.br",
  "version": "1.3.5",
  "main": "index.js",
  "author": "POP Pedidos",
  "license": "ISC",
  "description": "Faça suas vendas online, facilitando a divulgação de sua empresa e seus produtos na internet.",
  "scripts": {
    "start": "set NODE_ENV=development&&electron . --enable-logging",
    "start:production": "set NODE_ENV=production&&electron . --enable-logging",
    "build": "electron-builder build --win --publish never",
    "publish": "electron-builder build --win --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaovitmac/pop-client.git"
  },
  "build": {
    "publish": [
      "github"
    ],
    "win": {
      "icon": "assets/images/logo.png",
      "target": {
        "target": "nsis",
        "arch": [
          "ia32",
          "x64"
        ]
      }
    }
  },
  "devDependencies": {
    "electron": "^11.4.8",
    "electron-builder": "^22.10.5",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "electron-log": "^4.3.5",
    "electron-reload": "^1.5.0",
    "electron-updater": "^4.3.9",
    "uuid": "^8.3.2"
  }
}
